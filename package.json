{
  "name": "setup-chromedriver",
  "version": "1.0.8",
  "private": true,
  "description": "setup-chromedriver",
  "main": "lib/setup-chromedriver.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanasess/setup-chromedriver.git"
  },
  "keywords": [
    "actions",
    "php",
    "setup"
  ],
  "author": "nanasess",
  "license": "GPL",
  "dependencies": {
    "@actions/core": "1.9.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.3",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "semver": "^7.3.7",
    "typed-rest-client": "^1.8.9"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^18.7.15",
    "@types/selenium-webdriver": "^4.1.3",
    "@types/semver": "^7.3.12",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "jest-circus": "^29.0.2",
    "prettier": "^2.7.1",
    "selenium-webdriver": "^4.4.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
