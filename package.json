    "@actions/github": "^6.0.0",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.2",
    "semver": "^7.7.1",
    "typed-rest-client": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.2.1",
    "@types/selenium-webdriver": "^4.1.28",
    "@types/semver": "^7.5.8",
    "@types/yauzl": "^2.10.3",
    "@vercel/ncc": "^0.38.3",
